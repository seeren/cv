{
  "name": "seeren",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack serve --open --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --recursive test/unit",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=lcovonly --reporter=text npm test",
    "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls"
  },
  "sass": "src/index",
  "main": "src/index",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.16.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-html-import-to-string": "^2.0.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^6.0.1",
    "nyc": "^15.1.0",
    "raw-loader": "^4.0.2",
    "sass-loader": "^12.3.0",
    "sinon": "^12.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "appable": "latest",
    "jsdom": "latest",
    "ui-design": "^3.0.0",
    "yaml": "^1.10.2"
  }
}
